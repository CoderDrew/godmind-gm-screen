{
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"color": {
			"palette": [
				{
					"slug": "base",
					"color": "#0a0e1a",
					"name": "Base"
				},
				{
					"slug": "surface",
					"color": "#1a1f2e",
					"name": "Surface"
				},
				{
					"slug": "surface-elevated",
					"color": "#252b3f",
					"name": "Surface Elevated"
				},
				{
					"slug": "primary",
					"color": "#00d9ff",
					"name": "Primary (Cyan)"
				},
				{
					"slug": "secondary",
					"color": "#ff00ff",
					"name": "Secondary (Magenta)"
				},
				{
					"slug": "accent",
					"color": "#7c3aed",
					"name": "Accent (Purple)"
				},
				{
					"slug": "text-primary",
					"color": "#e2e8f0",
					"name": "Text Primary"
				},
				{
					"slug": "text-secondary",
					"color": "#94a3b8",
					"name": "Text Secondary"
				},
				{
					"slug": "text-muted",
					"color": "#64748b",
					"name": "Text Muted"
				},
				{
					"slug": "success",
					"color": "#10b981",
					"name": "Success"
				},
				{
					"slug": "warning",
					"color": "#f59e0b",
					"name": "Warning"
				},
				{
					"slug": "error",
					"color": "#ef4444",
					"name": "Error"
				},
				{
					"slug": "overlay",
					"color": "rgba(10, 14, 26, 0.8)",
					"name": "Overlay"
				}
			],
			"gradients": [
				{
					"slug": "neon-glow",
					"gradient": "linear-gradient(135deg, #00d9ff 0%, #7c3aed 50%, #ff00ff 100%)",
					"name": "Neon Glow"
				},
				{
					"slug": "cyberpunk",
					"gradient": "linear-gradient(180deg, #1a1f2e 0%, #0a0e1a 100%)",
					"name": "Cyberpunk"
				},
				{
					"slug": "holographic",
					"gradient": "linear-gradient(45deg, #00d9ff 0%, #ff00ff 100%)",
					"name": "Holographic"
				}
			],
			"duotone": [
				{
					"colors": ["#0a0e1a", "#00d9ff"],
					"slug": "cyan-dark",
					"name": "Cyan and Dark"
				},
				{
					"colors": ["#0a0e1a", "#ff00ff"],
					"slug": "magenta-dark",
					"name": "Magenta and Dark"
				}
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"slug": "system",
					"name": "System Font"
				},
				{
					"fontFamily": "'JetBrains Mono', 'Courier New', monospace",
					"slug": "monospace",
					"name": "Monospace"
				},
				{
					"fontFamily": "'Inter', -apple-system, BlinkMacSystemFont, sans-serif",
					"slug": "primary",
					"name": "Primary"
				}
			],
			"fontSizes": [
				{
					"slug": "xs",
					"size": "0.75rem",
					"name": "Extra Small"
				},
				{
					"slug": "sm",
					"size": "0.875rem",
					"name": "Small"
				},
				{
					"slug": "base",
					"size": "1rem",
					"name": "Base"
				},
				{
					"slug": "md",
					"size": "1.125rem",
					"name": "Medium"
				},
				{
					"slug": "lg",
					"size": "1.25rem",
					"name": "Large"
				},
				{
					"slug": "xl",
					"size": "1.5rem",
					"name": "Extra Large"
				},
				{
					"slug": "2xl",
					"size": "2rem",
					"name": "2X Large"
				},
				{
					"slug": "3xl",
					"size": "2.5rem",
					"name": "3X Large"
				},
				{
					"slug": "4xl",
					"size": "3rem",
					"name": "4X Large"
				}
			]
		},
		"spacing": {
			"padding": true,
			"margin": true,
			"spacingSizes": [
				{
					"slug": "0",
					"size": "0",
					"name": "None"
				},
				{
					"slug": "xs",
					"size": "0.5rem",
					"name": "Extra Small"
				},
				{
					"slug": "sm",
					"size": "1rem",
					"name": "Small"
				},
				{
					"slug": "md",
					"size": "1.5rem",
					"name": "Medium"
				},
				{
					"slug": "lg",
					"size": "2rem",
					"name": "Large"
				},
				{
					"slug": "xl",
					"size": "3rem",
					"name": "Extra Large"
				},
				{
					"slug": "2xl",
					"size": "4rem",
					"name": "2X Large"
				},
				{
					"slug": "3xl",
					"size": "6rem",
					"name": "3X Large"
				}
			],
			"units": ["px", "em", "rem", "vh", "vw", "%"]
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1200px"
		},
		"border": {
			"radius": true,
			"color": true,
			"style": true,
			"width": true
		},
		"shadow": {
			"presets": [
				{
					"slug": "sm",
					"shadow": "0 1px 2px 0 rgba(0, 0, 0, 0.3)",
					"name": "Small"
				},
				{
					"slug": "md",
					"shadow": "0 4px 6px -1px rgba(0, 0, 0, 0.4), 0 2px 4px -1px rgba(0, 0, 0, 0.3)",
					"name": "Medium"
				},
				{
					"slug": "lg",
					"shadow": "0 10px 15px -3px rgba(0, 0, 0, 0.5), 0 4px 6px -2px rgba(0, 0, 0, 0.3)",
					"name": "Large"
				},
				{
					"slug": "neon-cyan",
					"shadow": "0 0 10px rgba(0, 217, 255, 0.5), 0 0 20px rgba(0, 217, 255, 0.3)",
					"name": "Neon Cyan Glow"
				},
				{
					"slug": "neon-magenta",
					"shadow": "0 0 10px rgba(255, 0, 255, 0.5), 0 0 20px rgba(255, 0, 255, 0.3)",
					"name": "Neon Magenta Glow"
				},
				{
					"slug": "neon-purple",
					"shadow": "0 0 10px rgba(124, 58, 237, 0.5), 0 0 20px rgba(124, 58, 237, 0.3)",
					"name": "Neon Purple Glow"
				}
			]
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--text-primary)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--primary)",
			"fontSize": "var(--wp--preset--font-size--base)",
			"lineHeight": "1.6"
		},
		"spacing": {
			"padding": {
				"top": "0",
				"right": "var(--wp--preset--spacing--md)",
				"bottom": "0",
				"left": "var(--wp--preset--spacing--md)"
			}
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--secondary)"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--4-xl)",
					"fontWeight": "700",
					"lineHeight": "1.2"
				},
				"color": {
					"text": "var(--wp--preset--color--text-primary)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--3-xl)",
					"fontWeight": "700",
					"lineHeight": "1.3"
				},
				"color": {
					"text": "var(--wp--preset--color--text-primary)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--2-xl)",
					"fontWeight": "600",
					"lineHeight": "1.4"
				},
				"color": {
					"text": "var(--wp--preset--color--text-primary)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xl)",
					"fontWeight": "600",
					"lineHeight": "1.4"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--lg)",
					"fontWeight": "600",
					"lineHeight": "1.5"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--md)",
					"fontWeight": "600",
					"lineHeight": "1.5"
				}
			},
			"button": {
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--base)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--base)",
					"fontWeight": "600"
				},
				"border": {
					"radius": "0.375rem"
				},
				"spacing": {
					"padding": {
						"top": "0.75rem",
						"right": "1.5rem",
						"bottom": "0.75rem",
						"left": "1.5rem"
					}
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--secondary)"
					},
					"shadow": "var(--wp--preset--shadow--neon-cyan)"
				}
			}
		},
		"blocks": {
			"core/post-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--3-xl)",
					"fontWeight": "700"
				},
				"color": {
					"text": "var(--wp--preset--color--text-primary)"
				}
			},
			"core/post-content": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--lg)",
						"bottom": "var(--wp--preset--spacing--lg)"
					}
				}
			},
			"core/navigation": {
				"color": {
					"background": "var(--wp--preset--color--surface)"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--md)",
						"right": "var(--wp--preset--spacing--md)",
						"bottom": "var(--wp--preset--spacing--md)",
						"left": "var(--wp--preset--spacing--md)"
					}
				}
			},
			"core/code": {
				"color": {
					"background": "var(--wp--preset--color--surface-elevated)",
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)"
				},
				"border": {
					"radius": "0.375rem"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--sm)",
						"right": "var(--wp--preset--spacing--sm)",
						"bottom": "var(--wp--preset--spacing--sm)",
						"left": "var(--wp--preset--spacing--sm)"
					}
				}
			}
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		},
		{
			"name": "sidebar",
			"title": "Sidebar",
			"area": "uncategorized"
		}
	],
	"customTemplates": [
		{
			"name": "blank",
			"title": "Blank",
			"postTypes": ["page", "post"]
		},
		{
			"name": "no-sidebar",
			"title": "No Sidebar",
			"postTypes": ["page", "post"]
		}
	]
}
